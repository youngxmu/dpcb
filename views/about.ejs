	</div>
	<script type="text/javascript">
		$(document).ready(function(){
			$.ajax({
				url : ctx + 'r/about',
				type : 'post',
				dataType : 'json',
				success : function(result){
					if(result.ret_code){
						$('.about-container').html(result.value);
